<article class="highlightslist <% if (pageID == 'home') { %>jumbotron hidden-xs<% } %>">
  <div class="container">
    <div class="row">
      <% highlights.forEach(function(item) { %>
        <div class="<% if (pageID == 'home') { %>col-lg-3 col-md-4 col-sm-6 col-xs-12 text-center<% } else { %>col-lg-7 col-md-6 col-sm-6<% } %>">
          <h3 class="highlightslist-title"><%= item.site %></h3>
          <h5 class="highlightslist-name">with<a href="/highlights/<%= item.id %>"></a></h5>
          <div class="highlightslist-info">
            <a href="/highlights/<%= item.id %>">

              <% if (pageID == 'home') { %>
                <img class="highlightslist-img img-circle" src="<%= item.image %>" alt="Photo of <%= item.site %>" width="200px" height="150px"></a>
                <% } %>

                <% if (pageID == 'highlightsList') { %>
                  <div class="container" style="width:850px;">
                    <div class="row">
                      <div class="col-md-6">
                        <img class="highlightslist-img img-circle pull-left" src="<%= item.image %>" alt="Photo of <%= item.site %>" width="400px" height="300px"></a>
                      </div>
                      <div class="col-md-6">
                        <iframe width="400" height="300" src="<%= item.videoEmbed %>"></iframe>
                      </div>
                    </div>
                  </div>
                  <% } %>
                </a>

                <% if (pageID == 'highlightDetail') { %>
                  <img class="highlightslist-img pull-left" src="<%= item.image %>" alt="Photo of <%= item.site %>" width="800px"></a>

                  <% } %>
                </a>

                <% if (pageID !== 'home') { %>
                  <% if (pageID == 'highlightsList') { %>
                    <% } else { %>
                      <div style="clear:both;">
                        <%- item.info %>
                      </div>
                      <div class="container" style="width:850px;">
                        <div class="row">
                          <div class="col-md-6">
                            <iframe width="400" height="300" src="<%= item.videoEmbed %>"></iframe>
                          </div>
                          <div id="map1" class="col-md-6">
                            <script>
                            function initMap() {
                              var centerPos = {lat: 46.802556, lng: 8.304693};
                              var map1 = new google.maps.Map(document.getElementById('map1'), {
                                center: centerPos,
                                zoom: 7,
                                mapTypeId: google.maps.MapTypeId.HYBRID,
                                draggable: true,
                              });
                              var marker1Pos = {lat: <%- item.lat -%>, lng: <%- item.long -%>};
                              var marker = new google.maps.Marker({
                                position: marker1Pos,
                                map: map1,
                                title: '<%- item.site -%>'
                              });
                              marker.addListener('click', function() {
                                var infowindow = new google.maps.InfoWindow({
                                  content: "<%= item.category %>",
                                  maxWidth: 400
                                });
                                infowindow.open(map1, marker);
                              });
                            };
                            </script>
                            <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC8Ty6i_atvbwQ3c66H_OpoaXYnBDNPaVc&callback=initMap"
                            async defer></script>
                          </div>
                        </div>
                        <% } %>
                        <% } %>
                      </div>
                    </div>
                    <%  }); %>
                  </div><!-- row -->
                </div><!-- container -->
              </article>
